{
  "name": "Anti Bad Words",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
 },
 "icons": {
   "48": "icons/icon48.png",
   "128": "icons/icon128.png"
 },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"],
      "css": ["main.css"]
    }
  ]
}
