{
  "name": "twitter Anti Curse",
  "description": "an extension that replace the curse word with some symbols",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
 },
 "icons": {
   "48": "icons/icon48.png",
   "128": "icons/icon128.png"
 },
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["index.js"],
      "css": ["main.css"]
    }
  ]
}
